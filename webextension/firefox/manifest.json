{
	"manifest_version": 2,
	"name": "ThinBridge",
	"version": "1.3.0",
	"description": "ThinBridge for Firefox",
	"permissions": [
		"nativeMessaging",
		"alarms",
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"icons": {
		"128": "misc/128x128.png"
	},
	"applications": {
		"gecko": {
			"id": "thinbridge@clear-code.com",
			"strict_min_version": "60.0"
		}
	}
}
